{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template creates the stack for project kfbawsnode. Generated by Sigma",
  "Parameters": {
    "lambdaCodeBucketParameter": {
      "Type": "String",
      "Description": "Name of the S3 bucket containing the Lambda deployment package"
    },
    "lambdaCodeKeyParameter": {
      "Type": "String",
      "Description": "S3 key of the Lambda deployment package"
    }
  },
  "Outputs": {
    "sqsKTestSQS": {
      "Description": "URL of SQS queue KTestSQS",
      "Value": "https://sqs.us-east-1.amazonaws.com/318300609668/KTestSQS"
    },
    "kfbawsnode": {
      "Description": "ARN of Lambda function kfbawsnode",
      "Value": {
        "Fn::GetAtt": [
          "kfbawsnode",
          "Arn"
        ]
      }
    }
  },
  "Resources": {
    "kfbawsnode": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "FunctionName": "kfbawsnode",
        "Description": "Invokes functions defined in kfbawsnode.js in project kfbawsnode. Generated by Sigma.",
        "Code": {
          "S3Bucket": {
            "Ref": "lambdaCodeBucketParameter"
          },
          "S3Key": {
            "Ref": "lambdaCodeKeyParameter"
          }
        },
        "MemorySize": 128,
        "Handler": "kfbawsnode.handler",
        "Timeout": 30,
        "Runtime": "nodejs10.x",
        "Layers": [],
        "TracingConfig": {
          "Mode": "PassThrough"
        },
        "Role": {
          "Fn::GetAtt": [
            "kfbawsnodeExecutionRole",
            "Arn"
          ]
        },
        "Environment": {
          "Variables": {
            "SIGMA_AWS_ACC_ID": {
              "Ref": "AWS::AccountId"
            }
          }
        }
      }
    },
    "kfbawsnodeExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "ManagedPolicyArns": [
          "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole"
        ],
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Action": [
                "sts:AssumeRole"
              ],
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              }
            }
          ]
        },
        "Policies": [
          {
            "PolicyName": "kfbawsnodePolicy",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": [
                    "dynamodb:Scan"
                  ],
                  "Resource": "arn:aws:dynamodb:us-east-1:318300609668:table/KChineseAnimal"
                },
                {
                  "Effect": "Allow",
                  "Action": [
                    "sqs:GetQueueAttributes",
                    "sqs:ReceiveMessage",
                    "sqs:DeleteMessage"
                  ],
                  "Resource": "arn:aws:sqs:us-east-1:318300609668:KTestSQS"
                }
              ]
            }
          }
        ]
      }
    },
    "sqsKTestSQSTriggerkfbawsnode0": {
      "Type": "AWS::Lambda::EventSourceMapping",
      "Properties": {
        "BatchSize": "10",
        "EventSourceArn": "arn:aws:sqs:us-east-1:318300609668:KTestSQS",
        "FunctionName": {
          "Ref": "kfbawsnode"
        }
      }
    },
    "sqsKTestSQSPermissionkfbawsnode": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "Action": "lambda:InvokeFunction",
        "FunctionName": {
          "Ref": "kfbawsnode"
        },
        "SourceArn": "arn:aws:sqs:us-east-1:318300609668:KTestSQS",
        "Principal": "sqs.amazonaws.com"
      }
    }
  }
}